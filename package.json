    "@vitest/coverage-istanbul": "^4.0.0",
    "ajv": "^8.17.1",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "eslint": "^9.28.0",
    "semantic-release": "^25.0.0",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "typescript-json-schema": "^0.67.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=20.6.0",
    "npm": ">=10.0.0"
